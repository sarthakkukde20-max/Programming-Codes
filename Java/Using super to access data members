class SuperMemberDemo {
    public static void main(String[] args) {
        B b = new B(12, 56);
        b.show();
    }
}

class A {
    int i;
}

class B extends A {
    int i, k;

    B(int a, int b) {
        super.i = a;   // parent class variable
        this.i = b;    // child class variable
    }

    void show() {
        System.out.println("super.i=" + super.i);
        System.out.println("this.i=" + this.i);
    }
}
